Funcion dia<- Ganancia (ingresos, gastos)
    dia <- ingresos - gastos
FinFuncion

Proceso Ganancia_dia
    Definir ing, gast, resultado Como Real
    Escribir "Ingrese ingresos:"
    Leer ing
    Escribir "Ingrese gastos:"
    Leer gast
    resultado <- Ganancia(ing, gast)
    Escribir "La ganancia neta es: ", resultado
FinProceso
