Funcion f <- CelsiusAFahrenheit(c)
    f <- (c * 9/5) + 32
FinFuncion

Proceso convercion_de_celcius_a_Fahrenheit
    Definir c, resultado Como Real
    Escribir "Ingrese temperatura en Celsius:"
    Leer c
    resultado <- CelsiusAFahrenheit(c)
    Escribir "En Fahrenheit es: ", resultado
FinProceso
